escription,\n            status: nextProps.workEditing.status,\n          });\n        }else{\n            this.setState({\n                id: '',\n                name_work: '',\n                userId: 1,\n                time: '',\n                description: '',\n                status: false\n            });\n        }\n      }\n\n    onChange = (e) => {\n        const target = e.target;\n        const name = target.name;\n        const value = target.value;\n        this.setState({\n            [name]: value,\n        });\n    }\n\n    onSave = (e) => {\n        const {workEditing} = this.props;\n        e.preventDefault();\n        if(workEditing && workEditing.id){\n            this.props.onUpdateWork(this.state);\n        }else{\n            this.props.onAddWork(this.state);\n        }\n    }\n    renderStatus(){\n        let html = null;\n        const {workEditing, classes} = this.props;\n        if(workEditing && workEditing.id){\n            html = (<FormControl className={classes.formControl} style={{width: '200px'}}>\n                        <InputLabel  id=\"demo-simple-select-label\">Trạng thái</InputLabel>\n                        <Select\n                            labelId=\"demo-simple-select-label\"\n                            id=\"demo-simple-select\"\n                            onChange={this.onChange} name=\"status\"\n                            value={this.state.status}\n                        >\n                        <MenuItem value={true}>Hoàn thành</MenuItem>\n                        <MenuItem value={false}>Chưa hoàn thành</MenuItem>\n                        </Select>\n                    </FormControl>);\n        }\n        return html;\n\n    };\n    render() {\n        const {name_work,time,description } = this.state;\n\n        const { open, handleClose, title ,workEditing} = this.props;\n\n        return (\n            <div>\n                <Dialog\n                    open={open}\n                    onClose={handleClose}\n                    aria-labelledby=\"form-dialog-title\"\n                >\n                    <DialogTitle id=\"form-dialog-title\">\n                        {title}\n                    </DialogTitle>\n                    <form onSubmit={this.onSave}>\n                        <DialogContent >\n                            <TextField\n                                autoFocus\n                                margin=\"dense\"\n                                id=\"name\"\n                                label=\"Name\"\n                                type=\"fullname\"\n                                required\n                                fullWidth\n                                name=\"name_work\"\n\n                                onChange={this.onChange}\n                                value={name_work}\n                            />\n                            <TextField\n                                margin=\"dense\"\n                                id=\"name1\"\n                                label=\"Description\"\n                                type=\"text\"\n                                required\n                                fullWidth\n                                name=\"description\"\n\n                                onChange={this.onChange}\n                                value={description}\n                            />\n                            <TextField\n                                margin=\"dense\"\n                                id=\"name2\"\n                                label=\"Time\"\n                                required\n                                type=\"number\"\n                                fullWidth\n                                name=\"time\"\n                                value={time}\n                                onChange={this.onChange}\n                            />\n\n                            {this.renderStatus()}\n                        </DialogContent>\n                        <DialogActions>\n                            <Button onClick={handleClose} color=\"primary\">\n                                Cancel\n                            </Button>\n                            <Button type=\"submit\" color=\"primary\">\n                                {workEditing?\"sửa công việc\" : \"thêm công việc\"}\n                            </Button>\n                        </DialogActions>\n                    </form>\n                </Dialog>\n            </div>\n        );\n    }\n}\nconst mapStateToProps = (state) => {\n    return {\n        workEditing: state.works.workEditing\n    };\n};\nconst mapDispatchToProps = (dispatch) => {\n    return {\n        onAddWork: (work) => dispatch(actionWorks.addWorks(work)),\n        onUpdateWork : (work) => dispatch(actionWorks.updateWorks(work))\n    };\n};\nexport default withStyles(styles)(\n    connect(mapStateToProps, mapDispatchToProps)(workForms))\n","/home/thienncb/Desktop/redux-saga/src/containers/Taskboard/styles.js",[],"/home/thienncb/Desktop/redux-saga/src/constansts/config.js",[],"/home/thienncb/Desktop/redux-saga/src/commons/axiosService.js",[],"/home/thienncb/Desktop/redux-saga/src/constansts/index.js",[],"/home/thienncb/Desktop/redux-saga/src/components/workitem/styles.js",[],"/home/thienncb/Desktop/redux-saga/src/actions/index.js",[],[],"/home/thienncb/Desktop/redux-saga/src/utils/apiCaller.js",[],"/home/thienncb/Desktop/redux-saga/src/components/workform/styles.js",[],"/home/thienncb/Desktop/redux-saga/src/components/globalloading/index.js",[],"/home/thienncb/Desktop/redux-saga/src/components/globalloading/style.js",[],"/home/thienncb/Desktop/redux-saga/src/reducers/loading.js",[],"/home/thienncb/Desktop/redux-saga/src/constansts/loading.js",[],"/home/thienncb/Desktop/redux-saga/src/actions/loading.js",[],"/home/thienncb/Desktop/redux-saga/src/components/search/index.js",[],"/home/thienncb/Desktop/redux-saga/src/components/search/styles.js",[],"/home/thienncb/Desktop/redux-saga/src/reduc